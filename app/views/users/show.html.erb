<div class="container-fluid">

  <!------ Include the above in your HEAD tag ---------->


    <div class="col-md-4 col-lg-4" style="padding: 1%;">
    <div class="row">
      <div class="col-lg-12 col-sm-12">

        <div class="card hovercard">
          <div class="cardheader">

          </div>
          <div class="avatar">
            <img alt="" src="data:image/jpeg;base64,">
          </div>
          <div class="info">
            <div class="title">
              <%=@user.name%> <%=@user.surname%>
            </div>
            <div class="desc"><%=@user.university.name%></div>
            <div class="desc"><%=@user.unv_fac.faculty.name%></div>
            <div class="desc"><%=@user.fac_dep.department.name%></div>
          </div>

        </div>

      </div>

    </div>
  </div>


<br>

    <div class="col-md-8 col-lg-8" style="padding: 1%;">
      <div class="row">
        <div class="w3-container w3-teal">
          <h1 id="note_name">POSTLARIM</h1>
        </div>
      </div>

      <div id="a-tag" class="row">
        <% @user_posts.each do |post| %>
          <a unv="<%= post.university_id %>" fac="<%= post.unv_fac_id %>" dep="<%= post.fac_dep_id %>" cour="<%= post.dep_cour_id %>" href="/posts/<%= post.id %>">
            <div class="w3-third w3-row-padding w3-margin-top">
              <div class="w3-card">
                <img src="http://www.kinyu-z.net/data/wallpapers/180/1334806.jpg" style="width:100%">
                <div class="w3-container">
                  <h5>

                    <%= post.university.name %>&nbsp;&nbsp;<%= post.unv_fac.faculty.name %><br>
                    <%= post.fac_dep.department.name %>&nbsp;&nbsp;<%= post.dep_cour.course.name %>
                  </h5>
                </div>
              </div>
            </div>
          </a>
        <% end %>
      </div>
    </div>


  </div>